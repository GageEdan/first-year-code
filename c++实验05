#include<stdio.h>

int main() {
	
	int sum =200;
	int o, p, q;
	for (int c = 1; c < sum / 1 &&  c <= 50; c++) {
		o = c;
		for (int w = 1; w < sum / 4 &&  w + c <= 50; w++) {
			p = w;
			for (int m = 1; m < sum / 6&&m+w+c<=50; m++) {
				q = m;

			}
		}
	}
	
	


	printf("搬的总数是%d\n", sum);
	printf("男人有%d个\n女人有%d个\n小孩有%d个", 2 *q, 2 * p, 2 * o);
	return 0;
}
