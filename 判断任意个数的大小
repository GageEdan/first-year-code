#include<stdio.h>
#define N 50
int a[N];
void showarray(int n) {
	printf("处理后的数据为：");
	for (int i = 0; i < n; i++) {
		printf("%d  ", a[i]);
	}
	printf("\n\n");
}
int main() {
	int n;
	int max;
	while (1) {
		int c = 0;
		printf("请输入待处理数据个数：");
		scanf("%d", &n);
		printf("请依次输入数据：");
		for (int i = 0; i < n; i++) {
			scanf("%d", &a[i]);
		}
		int v = n;
		while (c < n - 1) {
			for (int i = 0; i < v - 1; i++) {
				if (a[i] > a[i + 1]) {
					max = a[i];
					a[i] = a[i + 1];
					a[i + 1] = max;
				}
			}
			c++;
			v--;
		}
		showarray(n);
	}	
	// 0 1 2 3 4 5 6 7
	// 7 2 3 5 9 7 2
	//             7  
	return 0;
}
